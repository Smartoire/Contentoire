{
  "expo": {
    "name": "Contentoire",
    "slug": "contentoire",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "contentoire",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.smartoire.contentoire",
      "googleServicesFile": "./config/GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./config/google-services.json",
      "package": "com.smartoire.contentoire",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f00a4f05-0e8e-4f3d-b8f0-fe29ca37d6ff"
      }
    },
    "owner": "Smartoire"
  }
}